      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "candidaturas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::candidatura.candidatura",
      "mappedBy": "candidato"
    },
    "competenzas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::competenza.competenza",
      "mappedBy": "candidato"
    },
    "messaggios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::messaggio.messaggio"
    }
  }
