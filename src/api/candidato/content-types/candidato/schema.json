{
  "kind": "collectionType",
  "collectionName": "candidatoes",
  "info": {
    "singularName": "candidato",
    "pluralName": "candidatoes",
    "displayName": "Candidato",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "nome": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "password": {
      "type": "password"
    },
